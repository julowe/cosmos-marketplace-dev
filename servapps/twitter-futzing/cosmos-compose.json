{
  "cosmos-installer": {},
  "minVersion": "0.9.0",
  "services": {
    "{ServiceName}": {
      "image": "ghcr.io/julowe/twitter-futzing:latest",
      "container_name": "{ServiceName}",
      "hostname": "{ServiceName}",
      "restart": "unless-stopped",
      "environment": [
        "PORT=8080",
        "SECRET_KEY={Passwords.0}",
        "GUNICORN_WORKERS=2"
      ],
      "labels": {
        "cosmos-force-network-secured": "true",
        "cosmos-auto-update": "true",
        "cosmos-icon": "https://julowe.github.io/cosmos-servapps-official/twitter-futzing/icon.png"
      },
      "healthcheck": {
        "test": [
          "CMD",
          "python",
          "-c",
          "import urllib.request; urllib.request.urlopen('http://localhost:8080/health')"
        ],
        "interval": 60,
        "timeout": 10,
        "retries": 1,
        "start_period": 20
      },
      "routes": [
        {
          "name": "{ServiceName}",
          "description": "Expose {ServiceName} to the web",
          "useHost": true,
          "target": "http://{ServiceName}:8080",
          "mode": "SERVAPP",
          "Timeout": 14400000,
          "ThrottlePerMinute": 12000,
          "BlockCommonBots": true,
          "SmartShield": {
            "Enabled": true
          }
        }
      ]
    }
  }
}
